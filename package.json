{
  "name": "lucky-coders-e-commerce",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite --port 3001",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --fix",
    "format": "prettier --write src/**/*.{js,ts,tsx,scss}",
    "test": "jest --coverage --verbose",
    "prepare": "husky install",
    "lint-staged": "lint-staged $STAGED_FILES"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": "prettier --write"
  },
  "dependencies": {
    "@commercetools/platform-sdk": "^5.0.0",
    "@commercetools/sdk-client-v2": "^2.2.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@hookform/resolvers": "^3.2.0",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "jest-environment-jsdom": "^29.6.2",
    "react": "^18.2.0",
    "react-datepicker": "^4.16.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.14.2",
